<% form_for @in_message do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :uid %><br />
    <%= f.text_field :uid %>
  </p>
  <p>
    <%= f.label :sender_id %><br />
    <%= f.text_field :sender_id %>
  </p>
  <p>
    <%= f.label :sent_at %><br />
    <%= f.datetime_select :sent_at %>
  </p>
  <p>
    <%= f.label :confirmed_at %><br />
    <%= f.datetime_select :confirmed_at %>
  </p>
  <p>
    <%= f.label :hex_cipher_key %><br />
    <%= f.text_field :hex_cipher_key %>
  </p>
  <p>
    <%= f.label :hex_cipher_iv %><br />
    <%= f.text_field :hex_cipher_iv %>
  </p>
  <p>
    <%= f.label :head_asset_id %><br />
    <%= f.text_field :head_asset_id %>
  </p>
  <p>
    <%= f.label :body_asset_id %><br />
    <%= f.text_field :body_asset_id %>
  </p>
  <p>
    <%= f.label :subject %><br />
    <%= f.text_field :subject %>
  </p>
  <p>
    <%= f.label :content %><br />
    <%= f.text_area :content %>
  </p>
  <p><%= f.submit %></p>
<% end %>
